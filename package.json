    "globals": "^15.4.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.38",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-beta.16",
    "vite": "^6.0.0",
